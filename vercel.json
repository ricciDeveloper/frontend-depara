{
  "version": 2,
  "name": "dexpara-frontend",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "API_BASE_URL": "https://backend-depara.onrender.com"
  }
}
